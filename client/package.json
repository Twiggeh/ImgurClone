{
	"name": "Imgur_Clone",
	"version": "0.0.1",
	"description": "I can graphql :D",
	"main": "app.js",
	"author": "Twiggeh",
	"license": "MIT",
	"scripts": {
		"debug": "node ./scripts/debug.js",
		"build": "node ./scripts/build.js",
		"analyze": "node ./scripts/analyze.js",
		"gql:codegen": "graphql-codegen --config gql.yml"
	},
	"eslintConfig": {
		"parser": "@typescript-eslint/parser",
		"env": {
			"browser": true,
			"es6": true,
			"node": true
		},
		"extends": [
			"eslint:recommended",
			"plugin:react/recommended",
			"plugin:security/recommended",
			"plugin:@typescript-eslint/recommended"
		],
		"globals": {
			"Atomics": "readonly",
			"SharedArrayBuffer": "readonly"
		},
		"parserOptions": {
			"ecmaFeatures": {
				"jsx": true
			},
			"ecmaVersion": 2018,
			"sourceType": "module"
		},
		"plugins": [
			"react",
			"react-hooks",
			"@emotion",
			"security"
		],
		"rules": {
			"indent": [
				"off",
				"tab",
				{
					"SwitchCase": 1
				}
			],
			"linebreak-style": [
				"error",
				"unix"
			],
			"quotes": [
				"error",
				"single"
			],
			"semi": [
				"error",
				"always"
			],
			"flatTernaryExpressions": 0,
			"react-hooks/rules-of-hooks": "error",
			"react-hooks/exhaustive-deps": "warn",
			"react/prop-types": 0,
			"no-unused-vars": "off",
			"no-var": "off",
			"@typescript-eslint/explicit-module-boundary-types": "off"
		},
		"settings": {
			"react": {
				"version": "detect"
			}
		},
		"ignorePatterns": [
			"node_modules/",
			"public/"
		]
	},
	"devDependencies": {
		"@apollo/client": "^3.3.6",
		"@babel/core": "^7.12.10",
		"@babel/plugin-transform-runtime": "^7.12.10",
		"@babel/preset-env": "^7.12.11",
		"@babel/preset-react": "^7.12.10",
		"@emotion/babel-preset-css-prop": "^11.0.0",
		"@emotion/core": "^11.0.0",
		"@emotion/eslint-plugin": "^11.0.0",
		"@emotion/react": "^11.0.27",
		"@emotion/styled": "^11.0.0",
		"@graphql-codegen/cli": "1.20.0",
		"@graphql-codegen/fragment-matcher": "2.0.1",
		"@graphql-codegen/introspection": "1.18.1",
		"@graphql-codegen/typescript": "1.19.0",
		"@graphql-codegen/typescript-document-nodes": "1.17.9",
		"@graphql-codegen/typescript-operations": "1.17.13",
		"@graphql-codegen/typescript-react-apollo": "2.2.1",
		"@hot-loader/react-dom": "^17.0.1",
		"@types/apollo-upload-client": "^14.1.0",
		"@types/node": "^14.14.27",
		"@types/react": "^17.0.0",
		"@types/react-dom": "^17.0.0",
		"@types/react-router-dom": "^5.1.6",
		"@types/uuid": "^8.3.0",
		"@typescript-eslint/eslint-plugin": "^4.10.0",
		"@typescript-eslint/parser": "^4.10.0",
		"babel-eslint": "^10.1.0",
		"babel-loader": "^8.2.2",
		"clean-webpack-plugin": "^3.0.0",
		"css-loader": "^5.0.1",
		"dotenv": "^8.2.0",
		"eslint": "^7.16.0",
		"eslint-loader": "^4.0.2",
		"eslint-plugin-emotion": "^11.0.0",
		"eslint-plugin-react": "^7.21.5",
		"eslint-plugin-react-hooks": "^4.2.0",
		"eslint-plugin-security": "^1.4.0",
		"eslint-webpack-plugin": "^2.4.1",
		"file-loader": "^6.2.0",
		"graphql": "^15.4.0",
		"html-loader": "^1.3.2",
		"html-webpack-plugin": "^4.5.0",
		"mini-css-extract-plugin": "^1.3.3",
		"optimize-css-assets-webpack-plugin": "^5.0.4",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-hot-loader": "^4.13.0",
		"react-router": "^5.2.0",
		"react-router-dom": "^5.2.0",
		"react-svg-loader": "^3.0.3",
		"regenerator-runtime": "^0.13.7",
		"resolve-url-loader": "^3.1.2",
		"style-loader": "^2.0.0",
		"ts-loader": "^8.0.12",
		"typescript": "^4.1.3",
		"uglifyjs-webpack-plugin": "^2.2.0",
		"url-loader": "^4.1.1",
		"uuid": "^8.3.2",
		"webpack": "^4.41.3",
		"webpack-bundle-analyzer": "^4.3.0",
		"webpack-cli": "^3.3.10",
		"webpack-dev-server": "^3.9.0"
	},
	"dependencies": {
		"apollo-upload-client": "^14.1.3",
		"cypress": "^6.4.0",
		"cypress-react-selector": "^2.3.6"
	}
}
